#include <msp430.h> 
#include <stdio.h>

/**
 * main.c
 */

float b = 5;

int main(void)
{

    int a = 4;

    unsigned char c = 0x06;
    unsigned char d = 0x07;
    unsigned char eq1 = c+1;
    unsigned char eq2 = d-1;

    a+=1; // increment
    b-=1; // decrement

    if ( eq1 && eq2 ) {
          printf("True\n" );
       }

    if ( eq1 || eq2 ) {
          printf("True\n" );
       }


    return 0;
}
